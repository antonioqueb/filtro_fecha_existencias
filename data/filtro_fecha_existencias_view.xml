<?xml version="1.0" encoding="UTF-8"?>

<odoo>

   <record id="filtro_fecha_existencias_view" model="ir.ui.view">
    <field name="name">filtro.fecha.existencias.form</field>
    <field name="model">filtro.fecha.existencias</field>
    <field name="arch" type="xml">
        <form>
            <group>
                <field name="date"/>
                <field name="location_id"/>
            </group>
            <field name="stock_data">
                <tree>
                    <field name="location_id"/>
                    <field name="product_id"/>
                    <field name="quantity"/>
                </tree>
            </field>
        </form>
    </field>
</record>


</odoo>
